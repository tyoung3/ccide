#: src/ccideinline.c:55
#, c-format
msgid "%s_COMMENT(%s Substitution strings are: %s and %s%s)\n"
msgstr "%s_COMMENT(%s Zeichenketten zur Ersetzung sind: %s und %s%s)\n"

#: src/ccideinline.c:128
#, c-format
msgid "%i exceeds maximum skeleton size:%i.  Resetting.\n"
msgstr "%i überschreitet die maximale Gerüstgröße %i. Es wird neu gestartet.\n"

#: src/ccideinline.c:134
#, c-format
msgid "%s Skeleton Program Generated by ccide-%s.%s %s \n"
msgstr "%s Gerüstprogramm generiert von ccide-%s.%s %s \n"

#: src/ccideinline.c:274
#, c-format
msgid "%sEnd of Skeleton Program.%s\n"
msgstr "%sEnde des Gerüstprogramms.%s\n"

#: src/ccideparse.c:1570
msgid "NEWGROUP not supported for GOTOless languages."
msgstr "NEWGROUP wird für Sprachen ohne GOTO nicht unterstützt."

#: src/ccideparse.c:1597
#, c-format
msgid "%i exceeds the maximum number of rules  %i."
msgstr "%i überschreitet die maximale Regelanzahl %i."

#: src/ccideparse.c:1602
#, c-format
msgid "%i exceeds the maximum number of actions %i."
msgstr "%i überschreitet die maximale Aktionsanzahl %i."

#: src/ccideparse.c:1607
#, c-format
msgid "%i exceeds the maximum number of conditions %i."
msgstr "%i überschreitet die maximale Bedingungsanzahl %i."

#: src/ccideparse.c:1873
#, c-format
msgid "%i rules, instead of %i."
msgstr "%i statt %i Regeln."

#: src/ccideparse.c:1888
#, c-format
msgid "Prefix longer than space allows."
msgstr "Der Präfix ist zu lang."

#: src/ccideparse.c:1906
#, c-format
msgid "Prefix contains invalid characters."
msgstr "Der Präfix beinhaltet ungültige Zeichen."

#: src/ccideparse.c:1934
#, c-format
msgid ""
"CCIDE/FATAL: Delimiter %s cannot equal a QUOTE=(%s,%s) or a SUBSTITUTION=(%s,"
"%s) \n"
msgstr ""
"CCIDE/FATAL: Trennzeichen %s kann nicht mit einem QUOTE=(%s,%s) oder einer "
"SUBSTITUTION=(%s,%s) übereinstimmen.\n"

#: src/ccideparse.c:1946
#, c-format
msgid "NULL Delimiter\n"
msgstr "NULL-Trennzeichen\n"

#: src/ccideparse.c:2122
#, c-format
msgid "CCIDE/PARSE: Sorry, %s programming language is not supported, yet.\n"
msgstr ""
"CCIDE/PARSE: Entschuldigung, die Programmiersprache %s wird noch nicht "
"unterstützt.\n"

#: src/ccideparse.c:2260
msgid "Binding gettext"
msgstr "Gettext wird gebunden."

#: src/ccideparse.c:2263
msgid "Getting textdomain"
msgstr "Textbereich wird geholt."

#: src/ccideparse.c:2278
#, c-format
msgid "Parameter %i is too long.\n"
msgstr "Parameter %i ist zu lang.\n"

#: src/ccidemain.c:186
#, c-format
msgid "Cannot find case value for rule %i%s\n"
msgstr "Es konnte kein Fallwert für die Regel %i%s gefunden werden.\n"

#: src/ccidemain.c:332
#, c-format
msgid "Dropping rule %i in table %i."
msgstr "Regel %i aus Tabelle %i wird gelöscht."

#: src/ccidemain.c:663
#, c-format
msgid "Rule %i conflicts with rule %2i"
msgstr "Regel %i widerspricht Regel %2i"

#: src/ccidemain.c:700
#, c-format
msgid "Rules %2i and  %2i overlap.\n"
msgstr "Regeln %2i und %2i überschneiden sich.\n"

#: src/ccidemain.c:773
msgid "Allocating leading white space"
msgstr "Führender Leerraum wird im Speicher zugewiesen."

#: src/ccidemain.c:820
#, c-format
msgid " '=' probably should be '==' in condition stub %i"
msgstr " ‚=‘ in Bedingungsabschnitt %i sollte vermutlich ‚==‘ heißen"

#: src/ccidemain.c:847
#, c-format
msgid "%s is the same as condition %i"
msgstr "%s ist dasselbe wie Bedingung %i."

#: src/ccidemain.c:935
#, c-format
msgid "%s is the same as action %i"
msgstr "%s ist dasselbe wie Aktion %i."
