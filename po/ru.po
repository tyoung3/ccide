#: src/ccidemain.c:847
#, c-format
msgid "%s is the same as condition %i"
msgstr "%s тоже, что и условие %i"

#: src/ccidemain.c:935
#, c-format
msgid "%s is the same as action %i"
msgstr "%s тоже, что и действие %i"

