#: src/ccideinline.c:55
#, c-format
msgid "%s_COMMENT(%s Substitution strings are: %s and %s%s)\n"
msgstr "%s_COMMENT(%s Các chuỗi thay thế là: %s và %s%s)\n"

#: src/ccideinline.c:128
#, c-format
msgid "%i exceeds maximum skeleton size:%i.  Resetting.\n"
msgstr "%i đạt đến kích thước \"khung sườn\" tối đa: %i. Đang đặt lại.\n"

#: src/ccideinline.c:134
#, fuzzy, c-format
msgid "%s Skeleton Program Generated by ccide-%s.%s %s \n"
msgstr "%s Chương trình KHUNG được tạo bởi ccide-%s.%s %s %s\n"

#: src/ccideinline.c:274
#, c-format
msgid "%sEnd of Skeleton Program.%s\n"
msgstr "%sKết thúc của chương trình KHUNG.%s\n"

#: src/ccideparse.c:1589
#, c-format
msgid "%i exceeds the maximum number of rules  %i."
msgstr "%i đạt đến số lượng \"quy tắc\" tối đa %i."

#: src/ccideparse.c:1594
#, c-format
msgid "%i exceeds the maximum number of actions %i."
msgstr "%i đạt đến số lượng \"hành vi\" tối đa %i."

#: src/ccideparse.c:1599
#, c-format
msgid "%i exceeds the maximum number of conditions %i."
msgstr "%i đạt đến số lượng \"điều kiện\" tối đa %i."

#: src/ccideparse.c:1864
#, c-format
msgid "%i rules, instead of %i."
msgstr "%i quy tắc, thay vì %i."

#: src/ccideparse.c:1877
msgid "Prefix longer than space allows."
msgstr "Tiền tố dài hơn không gian cho phép."

#: src/ccideparse.c:1898
#, c-format
msgid "Prefix contains invalid characters."
msgstr "Tiền tố chứa ký tự không hợp lệ."

#: src/ccideparse.c:1926
#, c-format
msgid ""
"CCIDE/FATAL: Delimiter %s cannot equal a QUOTE=(%s,%s) or a SUBSTITUTION=(%s,"
"%s) \n"
msgstr ""
"CCIDE/NGHIÊM-TRỌNG: Dấu giới hạn %s không bằng với QUOTE=(%s,%s) hoặc một "
"SUBSTITUTION=(%s,%s) \n"

#: src/ccideparse.c:1938
#, c-format
msgid "NULL Delimiter\n"
msgstr "Dấu giới hạn NULL\n"

#: src/ccideparse.c:2082
#, c-format
msgid "CCIDE/PARSE: Sorry, %s programming language is not supported, yet.\n"
msgstr ""
"CCIDE/PHÂN-TÍCH: Rất tiếc, ngôn ngữ lập trình %s vẫn chưa được hỗ trợ.\n"

#: src/ccideparse.c:2220
msgid "Binding gettext"
msgstr "Đang ràng bộc gettext"

#: src/ccideparse.c:2223
msgid "Getting textdomain"
msgstr "Đang lấy textdomain"

#: src/ccideparse.c:2238
#, c-format
msgid "Parameter %i is too long.\n"
msgstr "Tham số %i quá dài.\n"

#: src/ccidemain.c:183
#, c-format
msgid "Cannot find case value for rule %i%s\n"
msgstr "Không tìm thấy giá trị \"case\" cho quy tắc %i%s\n"

#: src/ccidemain.c:662
#, c-format
msgid "Rule %i conflicts with rule %2i\n"
msgstr "Quy tắc %i xung đột với quy tắc %2i\n"

#: src/ccidemain.c:699
#, c-format
msgid "Rules %2i and  %2i overlap.\n"
msgstr "Quy tắc %2i và  %2i chồng lên nhau.\n"

#: src/ccidemain.c:772
msgid "Allocating leading white space"
msgstr "Cấp phát khoảng trắng dẫn đầu"

#: src/ccidemain.c:821
#, c-format
msgid "%s '=' error in condition expression %i"
msgstr "%s '=' lỗi trong biểu thức điều kiện %i"

#: src/ccidemain.c:847
#, c-format
msgid "%s is the same as condition %i"
msgstr "%s là giống với điều kiện %i"

#: src/ccidemain.c:935
#, c-format
msgid "%s is the same as action %i"
msgstr "%s là giống với thao tác %i"
