#, c-format
msgid "%i exceeds maximum skeleton size:%i.  Resetting.\n"
msgstr "%i dépasse la taille maximum du squelette: %i. Ré-initialisation.\n"

#: src/ccideinline.c:134
#, c-format
msgid "%s Skeleton Program Generated by ccide-%s.%s %s \n"
msgstr "%s Programme squelette généré par ccide-%s.%s %s \n"

